<!DOCTYPE html>
<!-- Website template by freewebsitetemplates.com -->
<html>
<head>
	<meta charset="UTF-8">
	<title>Dive Sites - Bhaccasyoniztas Beach Resort Website Template</title>
	<link rel="stylesheet" href="css/style.css" type="text/css">
</head>
<body>
	<div id="background">
		<div id="page">
			{{> header}}
			</div>
			<div id="contents">
				<div class="box">
					<div>
						<div class="body">
							<h3>{{newsData[0].latestNews.title}}</h3>
							<ul>
							{{#each newsData[0].latestNews.items}}
								<li>
									<a href="/news">{{this.text}}</a>
								</li>
							{{/each}}					
							</ul>
							<h3>{{newsData[0].vacationTips.title}}</h3>
								<ul>
							{{#each newsData[0].vacationTips.items}}						
									<li>
										<a href="/news">{{this.text}}</a>
									</li>
							{{/each}}						
								</ul>
						</div>
					</div>
				</div>
			</div>
		</div>
		{{> footer}}
	</div>
</body>
</html>